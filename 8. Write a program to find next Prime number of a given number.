#include<stdio.h>

int main()
{
    int a,i,n,Flag=0;
    printf("Enter a number:");
    scanf("%d",&a);
    a++;
    for(n=a; 1 ;n++)
    {
        Flag=0;
      for(i=2;i<=n/2;i++)
    {
       if(n%i==0)
            Flag=1;
    }
    if(Flag==0)
    {
        printf("%d ",n);
        break;
    }


    }
    return 0;
}
